/* 정수 배열에서 정수 n의 배수로만 구성되어 있는 배열 구하기 */
import java.util.*;

class Solution {
    public List solution(int n, int[] numlist) {
        List<Integer> answer = new ArrayList<>();
        for (int i = 0; i < numlist.length; i++) {
            if (numlist[i] % n == 0) {
                answer.add(numlist[i]);
            }
        }
        return answer;
    }
}
