/* 정수 n이 제곱수일 경우 1을, 아닐 경우 2 반환하기 */
class Solution {
    public int solution(int n) {
        int answer = 2;
        for (int i = 1; i <= 1000; i++) {
            if (n == i * i) {
                answer = 1;
            } else {
                continue;
            }
        }
        return answer;
    }
}
