/* 두 배열의 요소를 비교하여 같은 원소의 개수 반환하기 */
class Solution {
    public int solution(String[] s1, String[] s2) {
        int answer = 0;
        for (int i = 0; i < s1.length; i++) {
            for (int j = 0; j < s2.length; j++) {
                if (s1[i].equals(s2[j])) {
                    answer += 1;
                }
            }
        }
        return answer;
    }
}
