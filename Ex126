/* 문자열의 모든 문자를 소문자로 바꾸고 사전 순으로 정렬한 문자열 반환하기 */
import java.util.*;

class Solution {
    public String solution(String my_string) {
        String answer = "";
        String[] arr = my_string.toLowerCase().split("");
        Arrays.sort(arr);
        for (int i = 0; i < arr.length; i++) {
            answer += arr[i];
        }
        return answer;
    }
}
