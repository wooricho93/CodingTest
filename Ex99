/* "*"의 높이와 길이를 1이라고 했을 때, 정수 n만큼의 높이와 길이를 가진 직각삼각형 출력하기 */
import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        for (int i = 0; i < n; i++) {
            for (int j = 0; j <= i; j++) {
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
